<h1>Edit Task</h1>

<%= form_for(@task) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :completed %>
  <%= f.check_box :completed %>
  <%= f.submit  %>
<% end %>

<%= link_to "see all tasks", tasks_path %>
<%= link_to "edit task", edit_task_path(@task) %>
